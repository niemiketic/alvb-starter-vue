{
  "name": "HelloWorld",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run serve",
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "lint:custom": "eslint --ext .js,.vue ./",
    "lint:critical": "eslint --no-eslintrc --config .eslintrc.critical.json ./src",
    "format": "npm run format:all ; npm run format:js",
    "format:all": "prettier ./**/*.{md,json,js,css,scss,vue} --write",
    "format:js": "eslint --fix --ext .js,.vue ./ ; exit 0",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "test": "npm run test:unit && npm run test:e2e",
    "build": "vue-cli-service build",
    "deploy:github": "push-dir --dir=dist --branch=gh-pages --remote=git@github.com:niemiketic/alvb-starter-vue.git"
  },
  "dependencies": {
    "babel-polyfill": "~6.26.0",
    "debug": "~3.1.0",
    "intl": "~1.2.5",
    "portal-vue": "~1.3.0",
    "register-service-worker": "~1.4.1",
    "vue": "~2.5.17",
    "vue-router": "~3.0.1",
    "vuetify": "~1.1.8",
    "vuex": "~3.0.1",
    "vuex-persistedstate": "~2.5.4",
    "vuex-router-sync": "~5.0.0",
    "whatwg-fetch": "~2.0.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~3.0.0-rc.10",
    "@vue/cli-plugin-e2e-nightwatch": "~3.0.0-rc.10",
    "@vue/cli-plugin-eslint": "~3.0.0-rc.10",
    "@vue/cli-plugin-pwa": "~3.0.0-rc.10",
    "@vue/cli-plugin-unit-jest": "~3.0.0-rc.10",
    "@vue/cli-service": "~3.0.0-rc.10",
    "@vue/eslint-config-airbnb": "~3.0.0-rc.10",
    "@vue/test-utils": "~1.0.0-beta.23",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "~23.4.2",
    "eslint": "~5.2.0",
    "eslint-config-airbnb-base": "~13.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "~2.13.0",
    "eslint-plugin-vue": "~4.7.1",
    "node-sass": "~4.9.0",
    "prettier": "~1.14.0",
    "push-dir": "^0.4.1",
    "sass-loader": "~7.1.0",
    "stylus": "~0.54.5",
    "stylus-loader": "~3.0.2",
    "vue-template-compiler": "~2.5.17"
  }
}
